async function startProgram(){
	await speak("Start", true);
	await scrollMatrixText("Start", {r:0, g: 255, b: 0}, 6, true)
	await delay(4)
	await roll(0,100, 1.5)
	await delay(2)
	setMainLed({r:0, g:0, b:255})
	await roll(90, 100,1)
	await delay(4)
	await Sound.Animal.Monkey.play(true)
	await delay(2)
	await roll(180, 100, .7)
	await delay(2)
	
	
}